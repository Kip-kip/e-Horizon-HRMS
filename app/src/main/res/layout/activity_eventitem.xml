<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/edittextbg">

    <com.google.android.material.appbar.AppBarLayout
        android:id="@+id/app_bar_layout"
        android:fitsSystemWindows="true"
        android:layout_width="fill_parent"
        android:layout_height="0.0dip"
        app:elevation="0.0dip"
        android:background="@color/colorPrimaryDark"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">
        <androidx.appcompat.widget.Toolbar
            android:id="@+id/app_toolbar"
            android:fitsSystemWindows="true"
            android:layout_width="fill_parent"

            android:layout_height="wrap_content"
            android:minHeight="?actionBarSize"
            >

            <ImageView
                android:id="@+id/back"
                android:layout_marginTop="@dimen/_10sdp"
                android:layout_width="@dimen/_50sdp"
                android:layout_height="@dimen/_30sdp"
                android:src="@drawable/kq"/>





            <ImageView
                android:id="@+id/profileimage"
                android:layout_width="@dimen/_25sdp"
                android:layout_marginTop="@dimen/_10sdp"
                android:layout_marginRight="@dimen/_15sdp"
                android:layout_height="@dimen/_25sdp"
                android:layout_gravity="right"
                android:scaleType="fitXY"
                android:src="@drawable/ic_notifications_black_24dp"
                />



            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">



                <LinearLayout
                    android:id="@+id/imageView_navigator_save"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:layout_weight="1">



                    <ImageView
                        android:id="@+id/notifications"
                        android:layout_gravity="center"
                        android:layout_marginTop="@dimen/_10sdp"
                        android:layout_marginRight="@dimen/_5sdp"
                        android:layout_width="wrap_content"
                        android:layout_height="@dimen/_35sdp"
                        android:src="@drawable/hrmslogo"/>

                </LinearLayout>
            </LinearLayout>





        </androidx.appcompat.widget.Toolbar>

    </com.google.android.material.appbar.AppBarLayout>


    <LinearLayout
        android:layout_marginTop="@dimen/_50sdp"
        android:layout_width="match_parent"
        android:id="@+id/layout"
        android:orientation="vertical"
        android:background="@color/edittextbg"
        android:layout_height="match_parent">


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/_10sdp"
            android:layout_marginRight="@dimen/_10sdp"
            android:layout_marginTop="@dimen/_20sdp"
            android:background="@drawable/curve_alledges_white"
            android:orientation="vertical">



            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:layout_marginLeft="@dimen/_10sdp"
                android:padding="@dimen/spacing_large">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/black"
                    android:textStyle="bold"
                    android:fontFamily="@font/raleway_regular"
                    android:layout_gravity="center"
                    android:text="Event name: "
                    android:textSize="16sp" />

                <TextView
                    android:id="@+id/txtEventItemName"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/black"
                    android:fontFamily="@font/raleway_regular"
                    android:layout_gravity="center"
                    android:text=""
                    android:textSize="16sp" />


            </LinearLayout>



            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:layout_marginLeft="@dimen/_10sdp"
                android:padding="@dimen/spacing_large">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/black"
                    android:textStyle="bold"
                    android:fontFamily="@font/raleway_regular"
                    android:layout_gravity="center"
                    android:text="Event type: "
                    android:textSize="16sp" />

                <TextView
                    android:id="@+id/txtEventType"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/black"
                    android:fontFamily="@font/raleway_regular"
                    android:layout_gravity="center"
                    android:text=""
                    android:textSize="16sp" />


            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:layout_marginLeft="@dimen/_10sdp"
                android:padding="@dimen/spacing_large">

                <TextView
                    android:id="@+id/txtEventName"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/black"
                    android:textStyle="bold"
                    android:fontFamily="@font/raleway_regular"
                    android:layout_gravity="center"
                    android:text="Start date: "
                    android:textSize="16sp" />

                <TextView
                    android:id="@+id/txtEventStartDate"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/black"
                    android:fontFamily="@font/raleway_regular"
                    android:layout_gravity="center"
                    android:text=""
                    android:textSize="16sp" />


            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:layout_marginLeft="@dimen/_10sdp"
                android:padding="@dimen/spacing_large">

                <TextView
                    android:id="@+id/txtEventName"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/black"
                    android:textStyle="bold"
                    android:fontFamily="@font/raleway_regular"
                    android:layout_gravity="center"
                    android:text="End date: "
                    android:textSize="16sp" />

                <TextView
                    android:id="@+id/txtEventEndDate"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/black"
                    android:fontFamily="@font/raleway_regular"
                    android:layout_gravity="center"
                    android:text=""
                    android:textSize="16sp" />


            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:layout_marginLeft="@dimen/_10sdp"
                android:padding="@dimen/spacing_large">

                <TextView

                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/black"
                    android:textStyle="bold"
                    android:fontFamily="@font/raleway_regular"
                    android:layout_gravity="center"
                    android:text="Venue: "
                    android:textSize="16sp" />

                <TextView
                    android:id="@+id/txtEventVenue"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/black"
                    android:fontFamily="@font/raleway_regular"
                    android:layout_gravity="center"
                    android:text=""
                    android:textSize="16sp" />


            </LinearLayout>



        </LinearLayout>
    </LinearLayout>





</androidx.constraintlayout.widget.ConstraintLayout>